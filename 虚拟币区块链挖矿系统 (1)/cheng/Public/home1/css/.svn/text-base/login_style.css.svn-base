*,body{
	margin:0px auto;

}
a{
	text-decoration:none;
}

.sx_head_logo{	
	width:850px;
	height: 80px;
	margin:0px auto;
	overflow: hidden;
}
.sx_head_logo img{	
	margin-top:24px;
}
.reg_head{	
	width:850px;
	height: 120px;
	margin:0px auto;
	overflow: hidden;
	background: #FFF;
}
.reg_head img{	
	margin-top:44px;
}
.reg_center{
	width:850px;
	height:480px;
	margin:0px auto;
}
.reg_login{
	width:800px;
	height:500px;
	margin:15px auto 0px auto;
	border:1px solid #EFEFEF; 
	background: #FFF;
//	margin-top: 15px;
	padding: 20px;
	
}
.reg_title{
	border-bottom: 2px solid #CCC;
	padding-bottom: 30px;
}
.reg_title span{
    display:block;
    float:left;
	font-size:24px;
	font-weight: bolder;
	color:#AAA;
}
.reg_title img{
	float:right;
}
.reg_login h2{
	font-size: 16px;
	color: #05A;
}
.reg_forms{
	padding-top:20px;
}
.reg_left{
	width:80px;
	text-align:right;
	float:left;
	font-size:14px;
	line-height: 41px;
}
.reg_input{
	margin-left:10px;
	float:left;
}
.reg_input input{
	width:200px;
	height:30px;
	border:1px solid #C0D0D8;
	border-radius:3px;
	padding-left:5px;
	box-shadow:0px 1px 2px rgba(20,100,140,0.2) inset;
	margin-bottom: 10px;
}
.reg_input input:focus{
	outline:none;
	box-shadow:1px 1px 2px rgba(0,0,0,0.2); 
	border:1px solid #A7B6C4;
	background:#FCFCFC;
}
#checkboxs{
    width:10px;
    height:10px;
}
.reg_input span{
	display: inline-block;
	width:80px;
	height: 40px;
	color:#05A;
	line-height: 20px;
	position: relative;
	top: -5px;
	left:5px;
}
#reg_sub{
	background:#7cc432;
	border:1px solid #4fae20;
	border-radius: 3px;
	width: 120px;
	height:33px;
	border:0px;
	font-size: 16px;
	font-weight: bolder;
	color:#FFF;
	box-shadow: none;
}
.reg_right{
	float:left;
	margin-left: 10px;
	font-size:14px;
	line-height: 41px;
	color:#AAA;
}
.reg_pwd{
	display: inline-block;
	margin-top:-12px;
	line-height: 22px;
	font-weight: 400;
}
.reg_pwd i{
	display: inline-block;
	width:30px;
	height: 5px;
	background: #CCC;
	margin-right: 3px;
}
.pwdlevel_1 i:first-child{
	background: #f57548;
}
.pwdlevel_2 i:first-child{
	background: blue;
}
.pwdlevel_2 i:nth-child(2){
	background: blue;
}
.pwdlevel_3 i{
	background: green;
}
.pwdlevel_1{
	color:#f57548;
}
.pwdlevel_2{
	color:blue;
}
.pwdlevel_3{
	color:green;
}
.inp_error_info{
	color:#e77008;
}
.inp_error_info span{
	display: inline-block;
	padding: 6px;
	margin-right: 5px;
	background: url("../images/icon/ico.png") no-repeat -50px -50px;
}
.inp_success_info span{
	display: inline-block;
	margin:9px 5px 0px 0px;
	padding: 6px;
	background: url("../images/icon/ico.png") no-repeat -50px 0px;
}
/****************************************登录页面样式****************************************/
.enter_main{
	width:850px;
	height:586px;
	position:relative;
	z-index:1;
}
/*登录框*/
.enter_enter{
	float:right;
	width:430px;
	height:auto;
	overflow: hidden;
	border:1px solid #CDCECB;
	background: #FFF;
	margin-top:20px; 
}
.enter_enters{
	float:right;
	width:430px;
	height:auto;
	overflow: hidden;
}
/*图片样式*/
.enter_picture{
		width:400px;
		height:432px;
		float:left;
}
/*账号登录样式*/
.enter_number{
	float:left;
	width:300px;
	height:50px;
	line-height: 50px;
	margin-left:20px;
}
.enter_number h2{
	font-size: 16px;
	color:#303328;
}
.enter_down{
	width:62px;
	height:80px;
	float:right;
}
.enter_main1{
	background-color:#f5f5f5; 
}
/*登录框的form表单*/
.enter_form{
	width:400px;
	margin-left:10px;
	margin-top:90px;  
}
/*每一个form表单样式*/
.enter_inp{
	width:270px;
	height:40px;
	border:1px solid rgb(225,225,225);
	border-radius:3px;
	text-indent:10px;
	line-height: 40px;
}
.enter_inp:focus{
	background: none repeat scroll 0 0 #FCFCFC;
	border: 1px solid #233dd7;
	box-shadow: 1px 1px 2px rgba(0, 0, 0, 0.2);
    outline: medium none;
}
.login_error{
	border: 1px solid red;
}
.enter_inp2{
	margin-left:38px; 
	margin-top:15px;
}
/*协议*/
.enter_check{
	margin-top:10px; 
	width:270px;
	height:20px;
	font: 13px/1.5 "Lucida Grande",tahoma,arial;
	color:#666;
	float:left;
	margin-left:3px; 
}
.enter_check a{
	color:#3f8bec;
}
.enter_error{
	height:30px;
	width:270px;
	position:absolute;
	top:78px;
	left:467px;
	line-height:30px;
	background:rgb(249,245,199);
	font-size:13px;
	border:1px solid rgb(218,214,153);
}
/*登录按钮样式*/
.enter_sub{
	background:#7cc432;
	border:1px solid #4fae20;
	border-radius: 3px;
	width: 120px;
	height:33px;
	border:0px;
	font-size: 16px;
	font-weight: bolder;
	color:#FFF;
	margin-top:20px; 
	margin-left:38px; 
}
/*登录下方的忘记密码，注册新账号*/
.enter_forget li{
	margin-top:40px; 
	width:70px;
	height:20px;
	float:right;
	list-style:none;
	font: 12px/1.5 "Lucida Grande",tahoma,arial;
	color:#666;
}
/*其它登录方式*/
.enter_else{
	clear:both;
	width:430px;
	height:47px;
	border-top:1px dashed rgb(204,204,204);
	line-height:47px;
	padding-left: 10px;
}
/*其它登录文字样式*/
.enter_way{
	float:left;
	width:75px;
	font: 12px/1.5 "Lucida Grande",tahoma,arial;
	line-height:47px;
	color:#666; 
}
.enter_way a{
	color:#3f8bec;
}
.enter_way1{
	width:80px;
	height:25px;
	float:left;
	margin-top:7px; 
}
.enter_zway{
	width:20px;
	height: 25px;
	float:left;
	text-align:left;
	margin-top:7px; 
}
.enter_empty{
	clear: both;
	width:400px;
	height:100px;
}
.login_foot{
	width:850px;
	height:65px;
	margin: 0px auto;
	text-align: center;
	padding-top: 15px;
	overflow: hidden;
	line-height: 25px;
}
/***********************协议页面的样式************************/
.agreement_nav{
	background:rgb(141,198,253);
}
.agreement_qq{
	margin:0px auto;
	width:958px;
	height:85px;
}
.agreement_content{
	width:958px;
	border:1px solid rgb(130,160,187);
	background:#ffffff;
}
.agreement_foot{
	width:958px;
	text-align:center;
	margin-top:10px;
}
.agreement_title{
	height:30px;
	text-align:center;
	line-height:30px;
	font-size:16px;
	margin-top:10px;
}
